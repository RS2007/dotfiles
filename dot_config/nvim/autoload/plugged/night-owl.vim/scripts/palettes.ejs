<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="application-name" content="haishanh/night-owl.vim" />
    <meta name="theme-color" content="#000000" />
    <title>haishanh/night-owl.vim</title>

    <style>
      body {
        margin: 0;
        padding: 0;
      }
      .ph10 {
        padding: 0 10%;
      }
      .flex {
        display: flex;
      }
      .flex-center {
        display: flex;
        justify-content: center;
        align-items: center;
      }
    </style>
  </head>
  <body>
    <div class="ph10">
      <h1>
        <code>haishanh/night-owl.vim</code> Color Palettes
      </h1>
    </div>
    <div class="flex ph10" style="flex-wrap: wrap;">
      <% palettes.forEach(function(c) { %>
        <div class="flex-center" style="background: <%= c.value %>;color: <%= c.nameColor %>;height:200px;width:200px"><%= c.name %></div>
      <% }) %>
    </div>
  </body>
</html>
